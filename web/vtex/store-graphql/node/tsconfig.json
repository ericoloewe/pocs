{
  "compilerOptions": {
    "types": [
      "bluebird-global",
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "esnext.asynciterable",
      "es2017"
    ],
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": false,
    "outDir": "service",
    "allowJs": true,
    "skipLibCheck": true
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": true
}
